<%= form_with(model: book) do |f| %>
	<div class="form-group">
    	<%= f.label :library_id %>
    	<%= f.number_field :library_id, value: params[:library], class:"form-control", readonly: true %>
	</div>
	<div class="form-group">
    	<%= f.submit "Confirm", class:"btn btn-primary" %>
  	</div>
<% end %>